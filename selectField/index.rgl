<overlay ref="overlay" class="u-selectField {class}" open={open} disabled={disabled} visible={visible} on-toggle="toggle">
    <overlay.head>
        <span>{#if _selected}{#inc _selected.$body}{/if}</span>
        <i class="u-icon u-icon-caret-down"></i>
    </overlay.head>
    <overlay.body>
        <ul class="m-listView selectField_listView">
            {#inc this.$body}
        </ul>
    </overlay.body>
</overlay>
